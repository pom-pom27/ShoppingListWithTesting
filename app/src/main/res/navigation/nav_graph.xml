<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/shoppingListFragment">

    <fragment
        android:id="@+id/shoppingListFragment"
        android:name="com.example.shoopinglistwithtesting.ui.fragments.ShoppingListFragment"
        android:label="ShoppingListFragment">
        <action
            android:id="@+id/action_shoppingListFragment_to_addShoppingFragment"
            app:destination="@id/addShoppingFragment" />
    </fragment>
    <fragment
        android:id="@+id/addShoppingFragment"
        android:name="com.example.shoopinglistwithtesting.ui.fragments.AddShoppingFragment"
        android:label="AddShoppingFragment">
        <action
            android:id="@+id/action_addShoppingFragment_to_imagePickFragment"
            app:destination="@id/imagePickFragment" />
    </fragment>
    <fragment
        android:id="@+id/imagePickFragment"
        android:name="com.example.shoopinglistwithtesting.ui.fragments.ImagePickFragment"
        android:label="ImagePickFragment" />
</navigation>
